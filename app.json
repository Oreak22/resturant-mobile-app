{
	"expo": {
		"name": "fashion-mobile",
		"slug": "fashion-mobile",
		"version": "1.0.0",
		"scheme": "fashion-mobile",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/splash-icon.png",
			"resizeMode": "cover",
			"backgroundColor": "#fafafa"
		},
		"ios": {
			"supportsTablet": true,
			"runtimeVersion": {
				"policy": "appVersion"
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.oreak.fashionmobile",
			"runtimeVersion": "1.0.0"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"fonts": {
			"Product Sans Regular": "./assets/fonts/Product Sans Regular.ttf",
			"Product Sans Bold": "./assets/fonts/Product Sans Bold.ttf"
		},
		"plugins": ["expo-router", "expo-font"],
		"extra": {
			"eas": {
				"projectId": "60ab9f30-71dd-4a7c-87b4-a0f70adc9dd7"
			}
		}
	}
}
